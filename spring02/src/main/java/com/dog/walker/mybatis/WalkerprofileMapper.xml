<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="walkerprofile">	

<insert id="insert" parameterType="walkerprofileDto">
      INSERT INTO walker_profile (id,career,experience,freeservice,imagePath,orgFileName,saveFileName,fileSize )
       VALUES(#{id},#{career}, #{experience}, #{freeservice},#{imagePath}, #{orgFileName}, #{saveFileName}, #{fileSize})
</insert>

<select id="getList" parameterType = "String" resultType="walkerprofileDto">
	SELECT p.* FROM pet_walker w,walker_profile p WHERE w.id = p.id AND addr LIKE '%' || SUBSTR(#{input_address},1,3) || '%'
</select>

</mapper>